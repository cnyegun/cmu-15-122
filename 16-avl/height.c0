

int tree_height(tree* T) {
	if (T == NULL) return 0;
	return 1 + max(tree_height(T->left), tree_height(T->right));
}

int tree_balance_factor(tree* T) {
	if (T == NULL) return 0;
	return tree_height(T->left) - tree_height(T->right);
}

